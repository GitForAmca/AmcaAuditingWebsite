@{
    IEnumerable<AMCAAuditing.Models.InsightModel> insightModel = ViewData["IndexInsight"] as IEnumerable<AMCAAuditing.Models.InsightModel>;
}
<div class="col-md-4 sticky order-2">
    <div class="sidebar-services">
        <h3><strong>Audit &amp; Assurance</strong></h3>
        <ul>
            <li><a href="@Url.Action("InternalAudit", "AuditAssuranceServices")" class="@ViewBag.InternalAudit">Internal Audit</a></li>
            <li><a href="@Url.Action("ExternalAudit", "AuditAssuranceServices")" class="@ViewBag.ExternalAudit">External Audit</a></li>
            <li><a href="@Url.Action("TaxAudit", "AuditAssuranceServices")" class="@ViewBag.TaxAudit">Tax Audit</a></li>
            <li><a href="@Url.Action("RevenueAudit", "AuditAssuranceServices")" class="@ViewBag.RevenueAudit">Revenue Audit</a></li>
            <li><a href="@Url.Action("InterimAudit", "AuditAssuranceServices")" class="@ViewBag.InterimAudit">Interim Audit</a></li>
        </ul>
        <h3><strong>Related Posts</strong></h3>
        <div class="row blogs-side-w">
            <div class="col-md-12 blogs-side">
                @foreach (var blogElement in insightModel)
                {
                    <div class="row bord">
                        <img src="@Url.Content(blogElement.ImageURL)" class="img-responsive" title="@blogElement.BlogTitle" alt="@blogElement.BlogTitle" />
                        <h4>
                            <a class="eachBlog" id="@blogElement.BlogID" href="/@blogElement.pageUrlText">@blogElement.BlogTitle</a>
                        </h4>
                        @*<p>by <span>@blogElement.CreatedBy</span></p>*@
                    </div>
                }

            </div>
        </div>
    </div>
</div>