<div class="col-md-4 sticky order-2">
    <div class="sidebar-services">
        <h3><strong>Recent Posts</strong></h3>
        <div class="row blogs-side-w">
            <div class="col-md-12 blogs-side">
                @foreach (var blogElement in Model.listData)
                {
                    <div class="row bord">
                        <img src="@Url.Content(blogElement.ImageURL)" class="img-responsive" title="@blogElement.BlogTitle" alt="@blogElement.BlogTitle" />
                        <h4>
                            <a class="eachBlog" id="@blogElement.BlogID" href="/@blogElement.pageUrlText">@blogElement.BlogTitle</a>
                        </h4>
                        @*<p>by <span>@blogElement.CreatedBy</span></p>*@
                    </div>
                }
               
            </div>
        </div>
    </div>
</div>